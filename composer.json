{
	"name": "fromholdio/silverstripe-grouploginredirect",
	"type": "silverstripe-vendormodule",
	"description": "A small SilverStripe module that allows assigning a page to a security group, forcing redirection to that page upon login for users in that group.",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "login", "groups", "redirect"],
	"homepage": "https://github.com/fromholdio/silverstripe-grouploginredirect",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-grouploginredirect/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
	"require": {
		"php": ">=5.6",
		"silverstripe/vendor-plugin": "^1.0",
		"silverstripe/cms": "^4.2"
	},
	"autoload": {
		"psr-4": {
			"Fromholdio\\GroupLoginRedirect\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
